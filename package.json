{
   "name": "Lettreinfo2",
   "version": "2.0.10",
   "description": "Convertir la lettre électronique fédérale",
   "homepage": "./",
   "main": "main.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "electron .",
      "dist": "electron-builder -wl"
   },
   "author": "SUD éducation",
   "license": "ISC",
   "devDependencies": {
      "electron": "^8.2.0",
      "electron-builder": "^22.4.1",
      "electron-packager": ">=14.2.1"
   },
   "dependencies": {
      "clean-css": ">=4.2.3",
      "html-minifier": ">=4.0.0",
      "string-strip-html": "^4.3.18"
   },
   "build": {
      "appId": "com.electron.lettreinfo",
      "asar": true,
      "linux": {
         "target": "snap",
         "target": "AppImage"
      },
      "win": {
         "target": "portable"
      },
      "mac": {
         "target": "dmg"
      }
   }
}
